<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Tab xmlns="http://javafx.com/javafx"
     xmlns:fx="http://javafx.com/fxml" fx:controller="ui.controllers.LogController" fx:id="serverLogTab"  onSelectionChanged="#loadData" closable="false"  styleClass="tab-main" text="Log">
    <content>
        <TableView
                fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="853.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
            <columns>
                <TableColumn fx:id="timeCol" prefWidth="75.0" text="Time" />
                <TableColumn fx:id="actionCol" prefWidth="75.0" text="Event" />
                <TableColumn fx:id="clientCol" maxWidth="2500.0" prefWidth="75.0" text="Client Info" />
                <TableColumn fx:id="descriptionCol" prefWidth="75.0" text="Description" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </content>
</Tab>

